<div class="jumbotron">
    <h1>DocGuard WebAPI</h1>
    <form method="post"
          enctype="multipart/form-data"
          action="http://localhost:7562/api/FileUploading/UploadFile">
        <div>
            <label for="doc">Please select an Office File</label>
            <input name="doc" type="file" class="btn btn-primary btn-lg">
        </div>
        <div>
            <input type="submit" value="Submit" class="btn btn-primary btn-lg">
        </div>
    </form>
</div>
